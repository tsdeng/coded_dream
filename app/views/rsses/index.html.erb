<h1>Listing rsses</h1>
<div class="rssEntryList">


<% @rsses.each do |rss| %>
  <div class="entryRow clearfix">
    <a href="<%=rss_path(rss)%>" class="title"><%=h rss.title %></a>
    <span class="address"><%=h rss.address %></span>

    <%= link_to 'Show', rss %>
    <%= link_to 'Edit', edit_rss_path(rss) %>
    <%= link_to 'Destroy', rss, :confirm => 'Are you sure?', :method => :delete %>
  </div>
<% end %>

</div>

<br />

<%= link_to 'New rss', new_rss_path %>